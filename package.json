{
  "name": "quick-credit-app",
  "version": "1.0.0",
  "description": "package dot json file for quick-credit-app",
  "main": "app.js",
  "scripts": {
    "start:server": "babel-node -- ./app.js",
    "dev": "nodemon --exec babel-node ./app.js",
    "build": "babel api --out-dir dist",
    "test": "mocha",
    "lint": "eslint .",
    "start": "babel-node ./app.js",
    "setup-db": "babel-node db.js --out-dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abolibot/Quick-Credit-App.git"
  },
  "author": "abolibot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abolibot/Quick-Credit-App/issues"
  },
  "homepage": "https://github.com/abolibot/Quick-Credit-App#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@hapi/joi": "^15.0.3",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "pg": "^7.11.0",
    "rand-token": "^0.4.0",
    "schm": "^0.4.1"
  },
  "engines": {
    "node": "10.15.1"
  }
}
